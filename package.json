{
  "name": "frontend.template.vue",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "prod": "npx vite --mode production",
    "build": "npx vue-tsc -b && npm run lint && npx vite build",
    "build:app": "npx vue-tsc --noEmit --skipLibCheck && npx vite build",
    "build:app:docker": "npx vite build",
    "build:test": "npx vue-tsc -b && npx vite build --mode test",
    "build:prod": "npx vue-tsc -b && npx vite build --mode production",
    "build:full": "npm run build:app && npm run build-storybook && npm run copy-storybook",
    "build:full:docker": "npm run build:app:docker && npm run build-storybook && npm run copy-storybook",
    "build:vercel": "npm run build:app:docker && npm run build-storybook && npm run copy-storybook",
    "copy-storybook": "node -e \"require('fs').cpSync('storybook-static', 'dist/storybook', {recursive: true, force: true})\"",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "type-check": "vue-tsc --noEmit",
    "preview": "npx vite preview",
    "preview:full": "npm run build:full && npx vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --quiet",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore --cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "husky || true"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.4",
    "i18n-iso-countries": "^7.14.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "sortablejs": "^1.15.6",
    "tinycolor2": "^1.6.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/language": "^6.11.2",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@cospired/i18n-iso-languages": "^4.2.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@types/node": "^22.16.5",
    "@types/sortablejs": "^1.15.8",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^9.33.0",
    "happy-dom": "^17.6.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "storybook": "^8.6.14",
    "storybook-vue3-router": "^5.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  }
}
